{
  "name": "fuzzy-search",
  "version": "1.0.0",
  "description": "Fuzzy search for Logseq with typo tolerance and relevance ranking",
  "main": "index.html",
  "scripts": {
    "build": "esbuild src/main.ts --bundle --outfile=dist/index.js --platform=browser --format=iife --global-name=LSPluginUser --external:@logseq/libs --target=chrome90"
  },
  "keywords": ["logseq", "search", "fuzzy-search"],
  "author": "Peter Denham",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "esbuild": "^0.27.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@logseq/libs": "^0.0.17",
    "minisearch": "^7.2.0"
  },
  "logseq": {
    "id": "fuzzy-search"
  }
}
